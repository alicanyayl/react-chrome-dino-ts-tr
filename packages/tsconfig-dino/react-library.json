{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "importHelpers": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "declarationMap": false
  }
}
